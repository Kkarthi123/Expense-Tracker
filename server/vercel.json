{
    "version": 2,
    "builds": [
      {
        "src": "app/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/server.js"
      }
    ],
    "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Set-Cookie",
          "value": "SameSite=None; Secure"
        }
      ]
  }
